{
  "version": 2,
  "name": "consultflow-frontend",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_DATA_SOURCE": "demo",
    "NEXT_PUBLIC_DEBUG_API": "false"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 25
    }
  },
  "regions": ["iad1"]
}